		var intDivision;
		//作成区分変更時

		function clickRadDivision(value) {
			$('btnSearch').disabled = true;
			if($(formID +'partnerNameUse') != null){
				$(formID +'partnerNameUse').checked = false;
			}
			intDivision = value;
			//入力フォームを見せているとき

			OrderFormDwr.getMSGInputInitConfirm(retGetMSGInputInitConfirm);

		}
		function retGetMSGInputInitConfirm(strMessage) {

			if (confirm(strMessage)) {
				OrderFormDwr.clickRadDivisionAction(intDivision, retClickRadDivision);
			} else {
				//フォーカスを戻す
				$('radDivision:'+0).checked = true;
				$('radDivision:'+0).focus();
			}
		}
		function retClickRadDivision() {
			if($('radDivision:0').checked == true) {
				$('btnSearch').disabled = true;
			}
			else if($('radDivision:1').checked == true) {
				$('btnSearch').disabled = false;
			}
			else if($('radDivision:2').checked == true) {
				$('btnSearch').disabled = false;
			}
			else if($('radDivision:3').checked == true) {
				$('btnSearch').disabled = false;
			}else  {
				$('btnSearch').disabled = true;
			}
			$('orderFormH:update').click();
		}

