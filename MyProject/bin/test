		function blurConstNo() {
			OrderFormDwr.checkConstNoChanged( $F(formID + 'txtConstNo'), {
				callback:function( befConstNo ) {
					if ( befConstNo != null ) {
						OrderFormDwr.checkKozokuSintyoku( {
							callback:function(isSintyoku) {
								if (isSintyoku) {
									OrderFormDwr.getMSGTyumonNoChangeFailure(showAlert);
									//戻す
									$( formID + 'txtConstNo').value = befConstNo;
								} else {
									OrderFormDwr.checkGenkaKeijo( {
										callback:function(strMessage) {
											if (strMessage != null) {
												if ( confirm( strMessage ) ) {
													checkTekigoDeletionConst(befConstNo);
												} else {
													//戻す
													$( formID + 'txtConstNo').value = befConstNo;
												}
											} else {
												checkTekigoDeletionConst(befConstNo);
											}
										}
									} );
								}
							}
						} );
					}
				}
			} );
		}
		function checkTekigoDeletionConst(befConstNo) {
			OrderFormDwr.hasTekigoMeisai( {
				callback:function(strMessage) {
					if (strMessage != null) {
						if ( confirm( strMessage ) ) {
							//クリアして実行
							OrderFormDwr.deleteAllTekigo();
							OrderFormDwr.setConstNo($F(formID + 'txtConstNo'));
							tableTekigoRefresh();
						} else {
							//戻す
							$( formID + 'txtConstNo').value = befConstNo;
						}
					} else {
						//実行
						OrderFormDwr.setConstNo($F(formID + 'txtConstNo'));
					}
				}
			} );
		}
		function blurSubjectCode() {
			OrderFormDwr.checkSubjectCodeChanged( $F(formID + 'txtSubjectCode'), {
				callback:function( befSubjectCode ) {
					if ( befSubjectCode != null ) {
						OrderFormDwr.checkKozokuSintyoku( {
							callback:function(isSintyoku) {
								if (isSintyoku) {
									OrderFormDwr.getMSGTyumonNoChangeFailure(showAlert);
									//戻す
									$( formID + 'txtSubjectCode').value = befSubjectCode;
								} else {
									OrderFormDwr.checkGenkaKeijo( {
										callback:function(strMessage) {
											if (strMessage != null) {
												if ( confirm( strMessage ) ) {
													checkTekigoDeletionSubject(befSubjectCode);
												} else {
													//戻す
													$( formID + 'txtSubjectCode').value = befSubjectCode;
												}
											} else {
												checkTekigoDeletionSubject(befSubjectCode);
											}
										}
									} );
								}
							}
						} );
					}
				}
			} );
		}
		function checkTekigoDeletionSubject(befSubjectCode) {
			OrderFormDwr.hasTekigoMeisai( {
				callback:function(strMessage) {
					if (strMessage != null) {
						if ( confirm( strMessage ) ) {
							//クリアして実行
							OrderFormDwr.deleteAllTekigo();
							OrderFormDwr.setSubjectCode($F(formID + 'txtSubjectCode'));
							tableTekigoRefresh();
						} else {
							//戻す
							$( formID + 'txtSubjectCode').value = befSubjectCode;
						}
					} else {
						//実行
						OrderFormDwr.setSubjectCode($F(formID + 'txtSubjectCode'));
					}
				}
			} );
		}

		//テーブルの適合状況を初期化します
		function tableTekigoRefresh() {
			OrderFormDwr.getMeisaiSavedDataSize( {
				callback:function(dataSize) {
					for ( i = 0; i < dataSize; i++) {
						$('kcell' + i + '_' + 11).value = "";	//納品日
						$('kcell' + i + '_' + 12).value = "";	//検品日
						$('kcell' + i + '_' + 13).value = '判定前';	//適合状況

					}
				}
			} );

		}
