		//印刷押下
		function clickBtnPrint() {
			try{
				printLog = document.getElementById('app').version;
				if(printLog == null | (((document.getElementById('app').major < 6)|(document.getElementById('app').major == 6 & document.getElementById('app').update < 10)) | document.getElementById('app').major > 7 )){
					if(printLog == null){
						printLog = 'error';
					}
					clickBtnPrintForImage();
				}else{
					OrderFormDwr.clickBtnPrintAction( {
						callback:function() {
							//プレビュー表示
							openReportPreview();
						}
					});
				}
			}catch (e) {
				printLog = '';
				jres = deployJava.getJREs();
				if(jres.length != 0){
					for(index = 0 ; index < jres.length ; index = index + 1){
						printLog = printLog.concat(new String(jres[index])).concat('/');
					}
				}
				if(printLog == ''){
					printLog = 'error';
				}
				clickBtnPrintForImage();

			}
			return false;
		}
