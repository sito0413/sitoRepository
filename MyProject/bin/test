		//注文番号書式変更時
		function changeLstOrderFormat() {
			var intIndex = $( formID + 'listOrderFormat').selectedIndex;
			//書式選択を送信
			OrderFormDwr.changeLstOrderFormatAction(intIndex, retChangeLstOrderFormat);
		}
		function retChangeLstOrderFormat(intDataCount) {

			var intIndex = $( formID + 'listOrderFormat').selectedIndex;
			var isChecked = $( formID + 'chkAllocateState').checked;

			//計上チェック && 複数に変更時
			if (isChecked && (intIndex == 1)) {

				OrderFormDwr.getMSGNotPlurals(showAlert);
				$( formID + 'listOrderFormat').selectedIndex = 0
				return;
			}

			//明細1行以上入力済み
			if (intDataCount > 0) {
				OrderFormDwr.getMSGFormatNotChanged(alert);

				if (intIndex == 0) {
					$( formID + 'listOrderFormat').selectedIndex = 1;
				} else {
					$( formID + 'listOrderFormat').selectedIndex = 0;
				}
			} else {

				if (intIndex == 1) {
					$( formID + 'txtConstNo').value = "";
					$( formID + 'txtSubjectCode').value = "";
					$( formID + 'txtGenkaRiyu').value = "";
				}

				checkShosiki();
			}
		}
		//注文書書式の活性チェック
		function checkShosiki() {
			var intIndex = $( formID + 'listOrderFormat').selectedIndex;
			//工番、科目コード
			if (intIndex == 0) {
				$(formID + 'txtConstNo').disabled = false;
				$(formID + 'txtSubjectCode').disabled = false;
				$(formID + 'txtGenkaRiyu').readOnly = false;
				$('btnSubjectCode').disabled = false;
				$('btnConstNumber').disabled = false;
			} else {
				$(formID + 'txtConstNo').disabled = true;
				$(formID + 'txtSubjectCode').disabled = true;
				$(formID + 'txtGenkaRiyu').readOnly = true;
				$('btnSubjectCode').disabled = true;
				$('btnConstNumber').disabled = true;
			}
		}
