		//支払い方法活性切り替え
		function clickRadPayment(id) {
			//選択区分を送信
			intKubun = id.substring(id.length-1,id.length);
			OrderFormDwr.clickRadPaymentAction(intKubun);

			if (intKubun == 0) {
				$(formID + 'txtCashPaymentDay').readOnly = true;
				$(formID + 'txtCashTantoPaymentDay').readOnly = true;
				$(formID + 'txtDepositPaymentDay').readOnly = true;
				$(formID + 'txtPaymentOther').readOnly = true;
				$(formID + 'txtCashPaymentDay').value = "";
				$(formID + 'txtCashTantoPaymentDay').value = "";
				$(formID + 'txtDepositPaymentDay').value = "";
				$(formID + 'txtPaymentOther').value = "";
			} else if (intKubun == 1) {
				$(formID + 'txtCashPaymentDay').readOnly = false;
				$(formID + 'txtCashTantoPaymentDay').readOnly = false;
				$(formID + 'txtDepositPaymentDay').readOnly = true;
				$(formID + 'txtPaymentOther').readOnly = true;
				$(formID + 'txtDepositPaymentDay').value = "";
				$(formID + 'txtPaymentOther').value = "";
			} else if (intKubun == 2) {
				$(formID + 'txtCashPaymentDay').readOnly = true;
				$(formID + 'txtCashTantoPaymentDay').readOnly = true;
				$(formID + 'txtDepositPaymentDay').readOnly = false;
				$(formID + 'txtPaymentOther').readOnly = true;
				$(formID + 'txtCashPaymentDay').value = "";
				$(formID + 'txtCashTantoPaymentDay').value = "";
				$(formID + 'txtPaymentOther').value = "";
			} else if (intKubun == 3) {
				$(formID + 'txtCashPaymentDay').readOnly = true;
				$(formID + 'txtCashTantoPaymentDay').readOnly = true;
				$(formID + 'txtDepositPaymentDay').readOnly = true;
				$(formID + 'txtPaymentOther').readOnly = false;
