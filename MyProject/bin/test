		//注文書属性変更
		function changeLstAttribute() {
			var intZouseiID = $F(formID + 'listOrderAttribute');
			OrderFormDwr.changeLstAttributeAction(intZouseiID, retChangeLstAttribute);
		}
		function retChangeLstAttribute(objZokuseiEntity) {
			setAttribute(objZokuseiEntity);

			$(formID + 'txtAreaOther').value = objZokuseiEntity.otherText;
			$(formID + 'txtAreaOther2').value = objZokuseiEntity.otherText2;

			$(formID + 'chkReserv1').checked = false;
			$(formID + 'chkReserv2').checked = false;
			$(formID + 'chkReserv3').checked = false;
		}
		function setAttribute(objZokuseiEntity) {

			var INT_DISABLE = 0;

			// 予約項目テキスト
			$('labReserv1').innerText = objZokuseiEntity.yoyakuName1;
			$('labReserv2').innerText = objZokuseiEntity.yoyakuName2;
			$('labReserv3').innerText = objZokuseiEntity.yoyakuName3;

			//進行 or 閲覧の場合


			// その他項目
			 $(formID + 'txtAreaOther').readOnly = true;		//【2009/11/02 変更】
			if (objZokuseiEntity.otherFlg == INT_DISABLE) {
				$(formID + 'txtAreaOther2').readOnly = true;
			} else {
				$(formID + 'txtAreaOther2').readOnly = false;
			}

			//	予約項目
			if (objZokuseiEntity.yoyaku1Flg == INT_DISABLE) {
				$(formID + 'chkReserv1').disabled = true;
				$('labReserv1').disabled = true;
			} else {
				$(formID + 'chkReserv1').disabled = false;
				$('labReserv1').disabled = false;
			}
			if (objZokuseiEntity.yoyaku2Flg == INT_DISABLE) {
				$(formID + 'chkReserv2').disabled = true;
				$('labReserv2').disabled = true;
			} else {
				$(formID + 'chkReserv2').disabled = false;
				$('labReserv2').disabled = false;
			}
			if (objZokuseiEntity.yoyaku3Flg == INT_DISABLE) {
				$(formID + 'chkReserv3').disabled = true;
				$('labReserv3').disabled = true;
			} else {
				$(formID + 'chkReserv3').disabled = false;
				$('labReserv3').disabled = false;
			}

		}

