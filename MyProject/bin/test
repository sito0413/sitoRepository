		//分割計上チェック
		function checkBunkatsuAble() {
			var isChecked =  $(formID + 'chkAllocateState').checked;

			//分割計上チェック && 複数書式
			if (isChecked == true &&  $(formID + 'listOrderFormat').selectedIndex == 1) {
				//メッセージ
				OrderFormDwr.getMSGNotAllocateSchedule(alert);
				 $(formID + 'chkAllocateState').checked = false;
				 $('btnAllocate').disabled = true;
				 $(formID + 'listDivisionTimes').disabled = true;

			} else {
				 $('btnAllocate').disabled = !isChecked;
				 $(formID + 'listDivisionTimes').disabled = !isChecked;
			}
		}
		//分割計上削除チェック
		function checkBunkatsuDeleteAble() {
			OrderFormDwr.changeLstDivisionTimesAction(retChangeLstDivisionTimes);
		}
		var intBefDivCount;
		function retChangeLstDivisionTimes(intCount) {
			intBefDivCount = intCount;
			if (intCount != 0) {
				OrderFormDwr.getMSGScheduleDeleteConfirm(retMSGScheduleDeleteConfirm)
			}
		}
		function retMSGScheduleDeleteConfirm(strMessage) {
			if (confirm(strMessage)) {
				OrderFormDwr.deleteScheduleAction();
				if ($F(formID + 'listDivisionTimes') == intBefDivCount) {
					$(formID + 'listDivisionTimes').value = 0;
				}
			} else {
				//チェンジ前の値をセット
				 $(formID + 'listDivisionTimes').value = intBefDivCount;
				 $(formID + 'chkAllocateState').checked = true;
				 $(formID + 'listDivisionTimes').disabled = false;
				 $('btnAllocate').disabled = false;
			}
		}
